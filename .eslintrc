{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "plugins": [
    "import",
    "jsx-a11y",
    "react",
  ],
  "env": {
    "browser": true,
    "node": true,
  },
  "rules": {
    "jsx-a11y-href-no-hash": "off",
    "jsx-a11y/anchor-is-valid": ["warn", { "aspects": ["invalidHref"] }],
  },
}
